{
    "name": "x3group-dev/bitrix24-api-laravel",
    "description": "b24 rest api",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "x3Group",
            "email": "dev@x3group.ru"
        },
        {
            "name": "MrDeff",
            "email": "e.pedan@gmail.com",
            "role": "Developer"
        },
        {
            "name": "Maxim Yugov",
            "email": "maxim.s.yugov@gmail.com",
            "role": "Developer"
        }
    ],
    "keywords": ["Laravel", "Bitrix", "Bitrix24"],
    "require": {
        "illuminate/support": "^11.0|^12.0",
        "bitrix24/b24phpsdk": "^1.1"
    },
    "require-dev": {
        "phpunit/phpunit": "~9.0",
        "orchestra/testbench": "~7"
    },
    "autoload": {
        "psr-4": {
            "X3Group\\Bitrix24\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "X3Group\\Bitrix24\\Tests\\": "tests"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "X3Group\\Bitrix24\\Bitrix24ServiceProvider"
            ],
            "aliases": {
                "Bitrix24ApiLaravel": "Bitrix24"
            }
        }
    }
}
